{
  "name": "Keybase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "reactbundle-ios": "react-native bundle --minify --root react --out ios/Keybase/main.jsbundle",
    "gobuild-ios": "gomobile bind -target=ios -o ios/keybase.framework github.com/keybase/client/go/loopback",
    "gobuild-android": "gomobile bind -target=android -o android/keybaselib/keybaselib.aar github.com/keybase/client/go/loopback",
    "packager-wipe-cache": "node ./react/packager/wipe-cache.js"
  },
  "dependencies": {
    "buffer": "^3.4.3",
    "iced-runtime": "^1.0.3",
    "react-native": "^0.11.0",
    "react-redux": "^2.1.2",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0",
    "util": "^0.10.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore":[
      "purepack/",
      "iced-error/",
      "framed-msgpack-rpc/",
      "keybase_v1.js"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "redux-devtools": "^2.1.2",
    "redux-logger": "^1.0.8"
  }
}
