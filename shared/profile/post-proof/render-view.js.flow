/* @flow */
import {Component} from 'react'
import type {ProvablePlatforms} from '../../constants/types/more'
import type {IconType} from '../../common-adapters/icon'

export type PropsSubset = {
  platform: ProvablePlatforms,
  platformUserName: string,
  platformSubtitle?: ?string,
  descriptionText?: string,
  descriptionView?: React$Element<*>,
  proofText?: ?string,
  proofAction?: () => void,
  proofActionIcon?: IconType,
  proofActionText?: ?string,
  noteText?: string,
  onCancel: () => void,
  onCancelText?: ?string,
  onComplete: () => void,
  isOnCompleteWaiting?: boolean,
  errorMessage?: ?string,
}

export type Props = PropsSubset & {
  onCompleteText: string,
}

export default class Render extends Component<void, Props, void> { }
