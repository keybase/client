+
+template <class Left, class Right>
+constexpr char char_at_(
+    const Left& left,
+    std::size_t left_count,
+    const Right& right,
+    std::size_t right_count,
+    std::size_t i) noexcept {
+  return i < left_count
+      ? left[i]
+      : i < (left_count + right_count) ? right[i - left_count] : '\0';
