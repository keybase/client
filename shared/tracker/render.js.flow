/* @flow */

import {Component} from 'react'

import type {SimpleProofState} from '../constants/tracker'
import type {UserInfo} from '../common-adapters/user-bio'
import type {Proof} from '../common-adapters/user-proofs'

export type RenderPropsUnshaped = {
  currentlyFollowing: boolean,
  inviteLink?: ?string,
  isPrivate?: boolean,
  lastAction: ?('followed' | 'refollowed' | 'unfollowed' | 'error'),
  loggedIn: boolean,
  name?: string,
  nonUser?: ?boolean,
  onClose: () => void,
  onFollow: () => void,
  onIgnore: () => void,
  onRefollow: () => void,
  onUnfollow: () => void,
  proofs: Array<Proof>,
  reason: string,
  serviceName?: string,
  shouldFollow: ?boolean,
  state: SimpleProofState,
  trackerState: SimpleProofState,
  userInfo: ?UserInfo,
  username: string,
  waiting: boolean,
}

export type RenderProps = $Shape<RenderPropsUnshaped>

export default class Render extends Component<void, RenderProps, void> { }
