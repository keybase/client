    "apply-new-fonts": "yarn run _helper apply-new-fonts",
    "build-dev": "yarn run _helper build-dev",
    "build-main-thread": "yarn run _helper build-main-thread",
    "build-prod": "yarn run _helper build-prod",
    "build-profile": "yarn run _helper build-profile",
    "build-wpdll": "yarn run _helper build-wpdll",
    "build-pegjs": "yarn run _node -- markdown/generate-emoji-parser.js markdown/parser.pegjs",
    "debug-main": "yarn run _helper debug-main",
    "generate-font-project": "yarn run _helper generate-font-project",
    "help": "yarn run _helper help",
    "hot-server": "yarn run build-main-thread && yarn run build-wpdll && yarn run hot-server-only",
    "hot-server-dumb": "yarn run build-main-thread && yarn run build-wpdll && yarn run hot-server-dumb-only",
    "hot-server-only": "yarn run _helper hot-server",
    "hot-server-dumb-only": "yarn run _helper hot-server-dumb",
    "inject-code-prod": "yarn run _helper inject-code-prod",
    "inject-sourcemaps-prod": "yarn run _helper inject-sourcemaps-prod",
    "lint": "./node_modules/.bin/eslint --quiet .",
    "flow": "./node_modules/.bin/flow .",
    "local-visdiff": "yarn run _helper local-visdiff",
    "package": "yarn run _helper package --",
    "postinstall": "yarn run _helper postinstall",
    "render-screenshots": "yarn run _helper render-screenshots",
    "setup-debug-main": "yarn run _helper setup-debug-main",
    "start": "yarn run build-main-thread && yarn run build-wpdll && yarn run _helper start",
    "start-cold": "yarn run _helper start-cold",
    "start-hot": "yarn run _helper start-hot",
    "start-hot-debug": "yarn run _helper start-hot-debug",
    "start-prod": "yarn run _helper start-prod",
    "test": "jest --config=desktop/test/jest-config.json --coverage",
    "undiff-log": "yarn run _helper undiff-log",
    "updated-fonts": "yarn run _helper updated-fonts",
    "vendor-install": "node yarn-vendor.js",
    "vendor-update": "node yarn-vendor.js update",
    "rn-start": "./node_modules/react-native/packager/packager.sh --resetCache",
    "rn-gobuild-ios": "./react-native/gobuild.sh ios",
    "rn-gobuild-android": "./react-native/gobuild.sh android",
    "rn-packager-wipe-cache": "watchman watch-del-all && rm -rf node_modules && yarn && node ./react-native/wipe-cache.js",
    "rn-test-ios": "xcodebuild test -project ./react-native/ios/Keybase.xcodeproj -scheme 'Keybase For Test' -destination 'platform=iOS Simulator,name=iPhone 6s,OS=9.3'"
  },
