{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "isolatedModules": false,
    "noImplicitAny": true,
    "tsBuildInfoFile": "./.tsOuts/.tsOutStrict/cache",
    "outDir": "./.tsOuts/.tsOutStrict/emit",
    "paths": {
      "immutable": ["override-d.ts/immutable/index.d.ts"],
      "popper": ["override-d.ts/popper/index.d.ts"],
      "react-native": ["override-d.ts/react-native/index.d.ts"],
      "react-native-fast-image": ["override-d.ts/react-native-fast-image/index.d.ts"],
      "react-native-gesture-handler": ["override-d.ts/react-native-gesture-handler/index.d.ts"],
      "react-native-image-picker": ["override-d.ts/react-native-image-picker/index.d.ts"],
      "react-redux": ["override-d.ts/react-redux/index.d.ts"],
      "redux": ["override-d.ts/redux/index.d.ts"],
      "rn-fetch-blob": ["override-d.ts/rn-fetch-blob/index.d.ts"]
    },
    "typeRoots": [
      "./node_modules/@types/",
      "./custom-d.ts/"
    ]
  },
  "files": [],
  "include": [
    "globals.d.ts",
    "constants/types/chat2/common.tsx",
    "constants/types/rpc-*.tsx",
    "actions/*-gen.tsx",
    "common-adapters/icon.constants.tsx"
  ]
}
