// @flow
import {Component} from 'react'

export type Props = {
  icon?: boolean,
  title?: string,
  onClose?: () => void,
  style?: Object,
  children?: ?Object,
  windowDragging?: boolean,
  type: 'Default' | 'Strong',
}

export type DefaultProps = {
  type: $PropertyType<Props, 'type'>,
}

declare export default class Header
  extends Component<DefaultProps, Props, void> {
  static defaultProps: DefaultProps,
}
