// @flow
import React, {Component} from 'react'

export type Props = {|
  hoverColor?: any,
  children?: any,
  style?: Object,
  onClick?: ?(event: SyntheticEvent) => void,
  onPress?: void,
  onLongPress?: ?(event: SyntheticEvent) => void,
  underlayColor?: any,
  onPressIn?: ?() => void,
  onPressOut?: ?() => void,
  feedback?: boolean,
  // mobile only
  activeOpacity?: number,
  // desktop only
  onMouseOver?: ?(event: SyntheticEvent) => void,
  onMouseEnter?: ?() => void,
  onMouseLeave?: ?() => void,
  onMouseDown?: ?() => void,
  onMouseUp?: ?() => void,
|}

declare export default class ClickableBox extends Component<void, Props, void> {}
