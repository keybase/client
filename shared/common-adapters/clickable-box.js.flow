// @flow
import React, {Component} from 'react'

export type Props = {
  className?: ?string,
  hoverColor?: ?string,
  children?: any,
  style?: ?Object,
  onClick?: ?(event: SyntheticEvent<>) => void,
  onPress?: void,
  onLongPress?: ?(event: SyntheticEvent<>) => void,
  underlayColor?: ?string,
  onPressIn?: ?() => void,
  onPressOut?: ?() => void,
  feedback?: boolean,
  // mobile only
  activeOpacity?: number,
  // desktop only
  onMouseOver?: ?(event: SyntheticEvent<>) => void,
  // You must specify both onMouseEnter and onMouseLeave if you
  // specify one.
  onMouseEnter?: ?() => void,
  onMouseLeave?: ?() => void,
  onMouseDown?: ?() => void,
  onMouseUp?: ?() => void,
}

declare export default class ClickableBox extends Component<Props> {}
