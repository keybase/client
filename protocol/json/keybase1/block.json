    "archiveReferenceWithCount": {
      "request": [
        {
          "name": "folder",
          "type": "string"
        },
        {
          "name": "refs",
          "type": {
            "type": "array",
            "items": "BlockReference"
          }
        }
      ],
      "response": "DowngradeReferenceRes"
