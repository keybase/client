    "delReference": {
      "request": [
        {
          "name": "folder",
          "type": "string"
        },
        {
          "name": "ref",
          "type": "BlockReference"
        }
      ],
      "response": null
    },
    "archiveReference": {
      "request": [
        {
          "name": "folder",
