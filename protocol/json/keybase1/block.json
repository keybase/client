        }
      ],
      "response": "bytes"
    },
    "getUserQuotaInfo2": {
      "request": [],
      "response": "BlockQuotaInfo",
      "compression_type": "gzip"
    },
    "getTeamQuotaInfo2": {
      "request": [
        {
          "name": "tid",
          "type": "TeamID"
        }
      ],
      "response": "BlockQuotaInfo",
      "compression_type": "gzip"
    },
    "blockPing": {
      "request": [],
      "response": "BlockPingResponse"
    }
  },
  "namespace": "keybase.1"
}