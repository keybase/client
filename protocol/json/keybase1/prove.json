{
  "protocol": "prove",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    },
    {
      "path": "identify_common.avdl",
      "type": "idl"
    },
    {
      "path": "prove_common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "CheckProofStatus",
      "fields": [
        {
          "type": "boolean",
          "name": "found"
        },
        {
          "type": "ProofStatus",
          "name": "status"
        },
        {
          "type": "string",
          "name": "proofText"
        },
        {
          "type": "ProofState",
          "name": "state"
        }
      ]
    },
    {
      "type": "record",
      "name": "StartProofResult",
      "fields": [
        {
          "type": "SigID",
          "name": "sigID"
        }
      ]
    }
  ],
  "messages": {
    "startProof": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "service",
          "type": "string"
        },
        {
          "name": "username",
          "type": "string"
        },
        {
          "name": "force",
          "type": "boolean"
        },
        {
          "name": "promptPosted",
          "type": "boolean"
        },
        {
          "name": "auto",
          "type": "boolean"
        },
        {
          "name": "sigVersion",
          "type": [
            null,
            "SigVersion"
          ]
        }
      ],
      "response": "StartProofResult"
    },
    "checkProof": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "sigID",
          "type": "SigID"
        }
      ],
      "response": "CheckProofStatus"
    },
    "listProofServices": {
      "request": [],
      "response": {
        "type": "array",
        "items": "string"
      }
    },
    "validateUsername": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "service",
          "type": "string"
        },
        {
          "name": "remotename",
          "type": "string"
        }
      ],
      "response": null
    }
  },
  "namespace": "keybase.1"
}