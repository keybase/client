{
  "protocol": "NotifyTracking",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [],
  "messages": {
    "trackingChanged": {
      "request": [
        {
          "name": "uid",
          "type": "UID"
        },
        {
          "name": "username",
          "type": "string"
        },
        {
          "name": "isTracking",
          "type": "boolean"
        }
      ],
      "response": null,
      "notify": ""
    },
    "trackingInfo": {
      "request": [
        {
          "name": "uid",
          "type": "UID"
        },
        {
          "name": "followers",
          "type": {
            "type": "array",
            "items": "string"
          }
        },
        {
          "name": "followees",
          "type": {
            "type": "array",
            "items": "string"
          }
        }
      ],
      "response": null,
      "oneway": true
    }
  },
  "namespace": "keybase.1"
}