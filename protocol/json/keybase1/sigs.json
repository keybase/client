        },
        {
          "type": "string",
          "name": "key"
        },
        {
          "type": "string",
          "name": "body"
        }
      ]
    },
    {
      "type": "record",
      "name": "SigTypes",
      "fields": [
        {
          "type": "boolean",
          "name": "track"
        },
        {
          "type": "boolean",
