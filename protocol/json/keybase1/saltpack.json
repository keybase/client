        },
        {
          "type": "boolean",
          "name": "noSelfEncrypt"
        },
        {
          "type": "boolean",
          "name": "binary"
        },
        {
          "type": "int",
          "name": "saltpackVersion"
        },
        {
          "type": "boolean",
          "name": "useKBFSKeysOnlyForTesting"
