        {
          "type": {
            "type": "array",
            "items": "string"
          },
          "name": "recipients"
        },
        {
          "type": "boolean",
          "name": "signed"
        },
        {
          "type": "boolean",
          "name": "includeSelf"
        }
      ]
    },
    {
      "type": "record",
