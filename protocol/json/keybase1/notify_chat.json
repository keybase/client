{
  "protocol": "NotifyChat",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    },
    {
      "path": "github.com/keybase/client/go/protocol/chat1",
      "type": "idl",
      "import_as": "chat1"
    }
  ],
  "types": [
    {
      "type": "enum",
      "name": "ChatActivityType",
      "symbols": [
        "RESERVED_0",
        "INCOMING_MESSAGE_1"
      ]
    },
    {
      "type": "record",
      "name": "ChatActivity",
      "fields": [
        {
          "type": "ChatActivityType",
          "name": "ActivityType"
        },
        {
          "type": [
            null,
            "chat1.MessageFromServerOrError"
          ],
          "name": "IncomingMessage"
        }
      ]
    }
  ],
  "messages": {
    "NewChatActivity": {
      "request": [
        {
          "name": "uid",
          "type": "UID"
        },
        {
          "name": "activity",
          "type": "ChatActivity"
        }
      ],
      "response": null,
      "notify": ""
    }
  },
  "namespace": "keybase.1"
}