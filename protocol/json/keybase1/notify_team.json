{
  "protocol": "NotifyTeam",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "TeamChangeSet",
      "fields": [
        {
          "type": "boolean",
          "name": "membershipChanged"
        },
        {
          "type": "boolean",
          "name": "keyRotated"
        },
        {
          "type": "boolean",
          "name": "renamed"
        }
      ]
    }
  ],
  "messages": {
    "teamChanged": {
      "request": [
        {
          "name": "teamID",
          "type": "TeamID"
        },
        {
          "name": "teamName",
          "type": "string"
        },
        {
          "name": "latestSeqno",
          "type": "Seqno"
        },
        {
          "name": "changes",
          "type": "TeamChangeSet"
        }
      ],
      "response": null,
      "oneway": true
    }
  },
  "namespace": "keybase.1"
}