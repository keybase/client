{
  "protocol": "Kex2Provisionee2",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    },
    {
      "path": "ephemeral.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "Hello2Res",
      "fields": [
        {
          "type": "KID",
          "name": "encryptionKey"
        },
        {
          "type": "HelloRes",
          "name": "sigPayload"
        },
        {
          "type": "KID",
          "name": "deviceEkKID"
        }
      ]
    },
    {
      "type": "record",
      "name": "PerUserKeyBox",
      "fields": [
        {
          "type": "PerUserKeyGeneration",
          "name": "generation"
        },
        {
          "type": "string",
          "name": "box"
        },
        {
          "type": "KID",
          "name": "receiverKID",
          "jsonkey": "receiver_kid"
        }
      ],
      "lint": "ignore"
    }
  ],
  "messages": {
    "hello2": {
      "request": [
        {
          "name": "uid",
          "type": "UID"
        },
        {
          "name": "token",
          "type": "SessionToken"
        },
        {
          "name": "csrf",
          "type": "CsrfToken"
        },
        {
          "name": "sigBody",
          "type": "string"
        }
      ],
      "response": "Hello2Res"
    },
    "didCounterSign2": {
      "request": [
        {
          "name": "sig",
          "type": "bytes"
        },
        {
          "name": "ppsEncrypted",
          "type": "string"
        },
        {
          "name": "pukBox",
          "type": [
            null,
            "PerUserKeyBox"
          ]
        },
        {
          "name": "userEkBox",
          "type": [
            null,
            "UserEkBoxed"
          ]
        }
      ],
      "response": null
    }
  },
  "namespace": "keybase.1"
}