    {
      "type": "enum",
      "name": "TrackDiffType",
      "symbols": [
        "NONE_0",
        "ERROR_1",
        "CLASH_2",
        "REVOKED_3",
        "UPGRADED_4",
        "NEW_5",
        "REMOTE_FAIL_6",
        "REMOTE_WORKING_7",
        "REMOTE_CHANGED_8",
        "NEW_ELDEST_9",
        "NONE_VIA_TEMPORARY_10"
      ]
    },
    {
      "type": "record",
      "name": "TrackDiff",
      "fields": [
        {
          "type": "TrackDiffType",
