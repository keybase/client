{
  "protocol": "chatLocal",
  "imports": [
    {
      "path": "github.com/keybase/client/go/protocol/chat1",
      "type": "idl",
      "import_as": "chat1"
    },
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "MessageText",
      "fields": [
        {
          "type": "string",
          "name": "body"
        }
      ]
    },
    {
      "type": "record",
      "name": "MessageConversationMetadata",
      "fields": [
        {
          "type": "string",
          "name": "conversationTitle"
        }
      ]
    },
    {
      "type": "record",
      "name": "MessageEdit",
      "fields": [
        {
          "type": "chat1.MessageID",
          "name": "messageID"
        },
        {
          "type": "string",
          "name": "body"
        }
      ]
    },
    {
      "type": "record",
      "name": "MessageDelete",
      "fields": [
        {
          "type": "chat1.MessageID",
          "name": "messageID"
        }
      ]
    },
    {
      "type": "record",
      "name": "MessageAttachment",
      "fields": [
        {
          "type": "string",
          "name": "path"
        }
      ]
    },
    {
      "type": "variant",
      "name": "MessageBody",
      "switch": {
        "type": "chat1.MessageType",
        "name": "messageType"
      },
      "cases": [
        {
          "label": {
            "name": "TEXT",
            "def": false
          },
          "body": "MessageText"
        },
        {
          "label": {
            "name": "ATTACHMENT",
            "def": false
          },
          "body": "MessageAttachment"
        },
        {
          "label": {
            "name": "EDIT",
            "def": false
          },
          "body": "MessageEdit"
        },
        {
          "label": {
            "name": "DELETE",
            "def": false
          },
          "body": "MessageDelete"
        },
        {
          "label": {
            "name": "METADATA",
            "def": false
          },
          "body": "MessageConversationMetadata"
        }
      ]
    },
    {
      "type": "record",
      "name": "MessagePlaintext",
      "fields": [
        {
          "type": "chat1.MessageClientHeader",
          "name": "clientHeader"
        },
        {
          "type": {
            "type": "array",
            "items": "MessageBody"
          },
          "name": "messageBodies"
        }
      ]
    },
    {
      "type": "record",
      "name": "MessageInfoLocal",
      "fields": [
        {
          "type": "boolean",
          "name": "isNew"
        },
        {
          "type": "string",
          "name": "senderUsername"
        },
        {
          "type": "string",
          "name": "senderDeviceName"
        }
      ]
    },
    {
      "type": "record",
      "name": "Message",
      "fields": [
        {
          "type": "chat1.MessageServerHeader",
          "name": "serverHeader"
        },
        {
          "type": "MessagePlaintext",
          "name": "messagePlaintext"
        },
        {
          "type": [
            null,
            "MessageInfoLocal"
          ],
          "name": "info"
        }
      ]
    },
    {
      "type": "record",
      "name": "ThreadView",
      "fields": [
        {
          "type": {
            "type": "array",
            "items": "Message"
          },
          "name": "messages"
        },
        {
          "type": [
            null,
            "chat1.Pagination"
          ],
          "name": "pagination"
        }
      ]
    },
    {
      "type": "record",
      "name": "MessageSelector",
      "fields": [
        {
          "type": {
            "type": "array",
            "items": "chat1.MessageType"
          },
          "name": "MessageTypes"
        },
        {
          "type": [
            null,
            "string"
          ],
          "name": "Since"
        },
        {
          "type": "boolean",
          "name": "onlyNew"
        },
        {
          "type": "int",
          "name": "limit"
        },
        {
          "type": {
            "type": "array",
            "items": "chat1.ConversationID"
          },
          "name": "conversations"
        },
        {
          "type": "boolean",
          "name": "markAsRead"
        }
      ]
    },
    {
      "type": "record",
      "name": "ConversationInfoLocal",
      "fields": [
        {
          "type": "chat1.ConversationID",
          "name": "id"
        },
        {
          "type": "string",
          "name": "tlfName"
        },
        {
          "type": "string",
          "name": "topicName"
        },
        {
          "type": "chat1.TopicType",
          "name": "topicType"
        }
      ]
    },
    {
      "type": "record",
      "name": "ConversationLocal",
      "fields": [
        {
          "type": "chat1.ConversationID",
          "name": "id"
        },
        {
          "type": [
            null,
            "ConversationInfoLocal"
          ],
          "name": "info"
        },
        {
          "type": {
            "type": "array",
            "items": "Message"
          },
          "name": "messages"
        }
      ]
    }
  ],
  "messages": {
    "getInboxLocal": {
      "request": [
        {
          "name": "pagination",
          "type": [
            null,
            "chat1.Pagination"
          ]
        }
      ],
      "response": "chat1.InboxView"
    },
    "getThreadLocal": {
      "request": [
        {
          "name": "conversationID",
          "type": "chat1.ConversationID"
        },
        {
          "name": "markAsRead",
          "type": "boolean"
        },
        {
          "name": "pagination",
          "type": [
            null,
            "chat1.Pagination"
          ]
        }
      ],
      "response": "ThreadView"
    },
    "postLocal": {
      "request": [
        {
          "name": "conversationID",
          "type": "chat1.ConversationID"
        },
        {
          "name": "messagePlaintext",
          "type": "MessagePlaintext"
        }
      ],
      "response": null
    },
    "resolveConversationLocal": {
      "request": [
        {
          "name": "conversation",
          "type": "ConversationInfoLocal"
        }
      ],
      "response": {
        "type": "array",
        "items": "ConversationInfoLocal"
      }
    },
    "newConversationLocal": {
      "request": [
        {
          "name": "conversation",
          "type": "ConversationInfoLocal"
        }
      ],
      "response": "ConversationInfoLocal"
    },
    "updateTopicNameLocal": {
      "request": [
        {
          "name": "conversationID",
          "type": "chat1.ConversationID"
        },
        {
          "name": "newTopicName",
          "type": "string"
        }
      ],
      "response": null
    },
    "getMessagesLocal": {
      "request": [
        {
          "name": "selector",
          "type": "MessageSelector"
        }
      ],
      "response": {
        "type": "array",
        "items": "ConversationLocal"
      }
    },
    "completeAndCanonicalizeTlfName": {
      "request": [
        {
          "name": "tlfName",
          "type": "string"
        }
      ],
      "response": "CanonicalTlfName"
    }
  },
  "namespace": "keybase.1"
}