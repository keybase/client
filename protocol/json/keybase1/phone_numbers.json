{
  "protocol": "phoneNumbers",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "UserPhoneNumber",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone_number"
        },
        {
          "type": "boolean",
          "name": "verified",
          "jsonkey": "verified"
        },
        {
          "type": "UnixTime",
          "name": "ctime",
          "jsonkey": "ctime"
        }
      ],
      "doc": "Phone number support for TOFU chats."
    },
    {
      "type": "record",
      "name": "PhoneNumberAddedMsg",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone"
        }
      ]
    },
    {
      "type": "record",
      "name": "PhoneNumberVerifiedMsg",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone"
        }
      ]
    },
    {
      "type": "record",
      "name": "PhoneNumberSupersededMsg",
      "fields": [
        {
          "type": "PhoneNumber",
          "name": "phoneNumber",
          "jsonkey": "phone"
        }
      ]
    }
  ],
  "messages": {
    "addPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "verifyPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "code",
          "type": "string"
        }
      ],
      "response": null
    },
    "getPhoneNumbers": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        }
      ],
      "response": {
        "type": "array",
        "items": "UserPhoneNumber"
      }
    },
    "deletePhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        }
      ],
      "response": null
    },
    "setVisibilityPhoneNumber": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "phoneNumber",
          "type": "PhoneNumber"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    }
  },
  "namespace": "keybase.1"
}