{
  "protocol": "emails",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "EmailLookupResult",
      "fields": [
        {
          "type": "EmailAddress",
          "name": "email"
        },
        {
          "type": [
            null,
            "UID"
          ],
          "name": "uid"
        }
      ]
    }
  ],
  "messages": {
    "addEmail": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "email",
          "type": "EmailAddress"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "deleteEmail": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "email",
          "type": "EmailAddress"
        }
      ],
      "response": null
    },
    "editEmail": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "oldEmail",
          "type": "EmailAddress"
        },
        {
          "name": "email",
          "type": "EmailAddress"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "setPrimaryEmail": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "email",
          "type": "EmailAddress"
        }
      ],
      "response": null
    },
    "sendVerificationEmail": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "email",
          "type": "EmailAddress"
        }
      ],
      "response": null
    },
    "bulkLookupEmails": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "emailContacts",
          "type": {
            "type": "array",
            "items": "string"
          }
        }
      ],
      "response": {
        "type": "array",
        "items": "EmailLookupResult"
      }
    },
    "setVisibilityEmail": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "email",
          "type": "EmailAddress"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "setVisibilityAllEmail": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "visibility",
          "type": "IdentityVisibility"
        }
      ],
      "response": null
    },
    "getEmails": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        }
      ],
      "response": {
        "type": "array",
        "items": "Email"
      }
    }
  },
  "namespace": "keybase.1"
}