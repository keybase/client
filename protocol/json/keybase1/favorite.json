{
  "protocol": "favorite",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "Folder",
      "fields": [
        {
          "type": "string",
          "name": "name"
        },
        {
          "type": "boolean",
          "name": "private"
        },
        {
          "type": "boolean",
          "name": "notificationsOn"
        },
        {
          "type": "boolean",
          "name": "created"
        }
      ],
      "doc": "Folder represents a favorite top-level folder in kbfs.\n    This type is likely to change significantly as all the various parts are\n    connected and tested."
    },
    {
      "type": "record",
      "name": "FavoritesResult",
      "fields": [
        {
          "type": {
            "type": "array",
            "items": "Folder"
          },
          "name": "favoriteFolders"
        },
        {
          "type": {
            "type": "array",
            "items": "Folder"
          },
          "name": "ignoredFolders"
        },
        {
          "type": {
            "type": "array",
            "items": "Folder"
          },
          "name": "newFolders"
        }
      ]
    }
  ],
  "messages": {
    "favoriteAdd": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "folder",
          "type": "Folder"
        }
      ],
      "response": null,
      "doc": "Adds a folder to a user's list of favorite folders."
    },
    "favoriteIgnore": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "folder",
          "type": "Folder"
        }
      ],
      "response": null,
      "doc": "Removes a folder from a user's list of favorite folders."
    },
    "getFavorites": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        }
      ],
      "response": "FavoritesResult",
      "doc": "Returns all of a user's favorite folders."
    }
  },
  "namespace": "keybase.1"
}