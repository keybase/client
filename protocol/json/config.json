          "type": [
            null,
            "User"
          ],
          "name": "user"
        }
      ]
    },
    {
      "type": "record",
      "name": "SessionStatus",
      "fields": [
        {
          "type": "string",
          "name": "SessionFor"
        },
        {
          "type": "boolean",
          "name": "Loaded"
        },
        {
          "type": "boolean",
          "name": "Cleared"
        },
        {
          "type": "boolean",
          "name": "SaltOnly"
        },
        {
          "type": "boolean",
          "name": "Expired"
        }
      ]
    },
    {
      "type": "record",
