        {
          "type": "boolean",
          "name": "tsecCached"
        },
        {
          "type": "boolean",
          "name": "deviceSigKeyCached"
        },
        {
          "type": "boolean",
          "name": "deviceEncKeyCached"
        },
        {
          "type": "boolean",
          "name": "paperSigKeyCached"
        },
        {
          "type": "boolean",
          "name": "paperEncKeyCached"
        },
        {
          "type": "boolean",
          "name": "storedSecret"
        },
        {
          "type": "boolean",
          "name": "secretPromptSkip"
        },
        {
          "type": [
            null,
            "Device"
          ],
          "name": "device"
        },
        {
          "type": "string",
          "name": "logDir"
        },
        {
          "type": [
            null,
            "SessionStatus"
