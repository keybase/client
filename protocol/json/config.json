{
  "protocol": "config",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "GetCurrentStatusRes",
      "fields": [
        {
          "type": "boolean",
          "name": "configured"
        },
        {
          "type": "boolean",
          "name": "registered"
        },
        {
          "type": "boolean",
          "name": "loggedIn"
        },
        {
