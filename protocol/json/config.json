      "type": "record",
      "name": "ConfigValue",
      "fields": [
        {
          "type": "boolean",
          "name": "isNull"
        },
        {
          "type": [
            null,
            "boolean"
          ],
          "name": "b"
        },
        {
          "type": [
            null,
            "int"
          ],
          "name": "i"
        },
        {
          "type": [
            null,
            "string"
          ],
          "name": "s"
        },
        {
          "type": [
            null,
            "string"
          ],
          "name": "o"
        }
      ]
