        },
        {
          "type": "string",
          "name": "version"
        }
      ]
    },
    {
      "type": "record",
      "name": "PlatformInfo",
      "fields": [
        {
          "type": "string",
          "name": "os"
        },
        {
          "type": "string",
          "name": "arch"
        },
        {
          "type": "string",
          "name": "goVersion"
        }
      ]
    },
    {
      "type": "record",
      "name": "ExtendedStatus",
      "fields": [
        {
          "type": "boolean",
          "name": "standalone"
