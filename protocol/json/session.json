{
  "protocol": "session",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "Session",
      "fields": [
        {
          "type": "UID",
          "name": "uid"
        },
        {
          "type": "string",
          "name": "username"
        },
        {
          "type": "string",
          "name": "token"
        },
        {
          "type": "KID",
          "name": "deviceSubkeyKid"
        },
        {
          "type": "KID",
          "name": "deviceSibkeyKid"
        }
      ]
    }
  ],
  "messages": {
    "currentSession": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        }
      ],
      "response": "Session"
    }
  },
  "namespace": "keybase.1"
}