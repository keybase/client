{
  "protocol": "secretUi",
  "imports": [
    {
      "path": "passphrase_common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "SecretEntryArg",
      "fields": [
        {
          "type": "string",
          "name": "desc"
        },
        {
          "type": "string",
          "name": "prompt"
        },
        {
          "type": "string",
          "name": "err"
        },
        {
          "type": "string",
          "name": "cancel"
        },
        {
          "type": "string",
          "name": "ok"
        },
        {
          "type": "string",
          "name": "reason"
        },
        {
          "type": "boolean",
          "name": "useSecretStore"
        }
      ]
    },
    {
      "type": "record",
      "name": "SecretEntryRes",
      "fields": [
        {
          "type": "string",
          "name": "text"
        },
        {
          "type": "boolean",
          "name": "canceled"
        },
        {
          "type": "boolean",
          "name": "storeSecret"
        }
      ]
    }
  ],
  "messages": {
    "getPassphrase": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "pinentry",
          "type": "GUIEntryArg"
        },
        {
          "name": "terminal",
          "type": [
            null,
            "SecretEntryArg"
          ]
        }
      ],
      "response": "GetPassphraseRes"
    }
  },
  "namespace": "keybase.1"
}