@namespace("keybase.1")

protocol ephemeral {
  import idl "common.avdl";

  @typedef("int")
  record EkGeneration {}

  record DeviceEkMetadata {
    @jsonkey("device_ephemeral_dh_public")
    KID kid;
    @jsonkey("hash_meta")
    HashMeta hashMeta;
    EkGeneration generation;
  }

  record DeviceEk {
    bytes seed;
    EkGeneration generation;
    @jsonkey("hash_meta")
    HashMeta hashMeta;
  }
}
