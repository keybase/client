@namespace("keybase.1")

protocol NotifySaltpack {

  enum OperationType {
    ENCRYPT_0,
    DECRYPT_1,
    SIGN_2,
    VERIFY_3
  }

  void saltpackOperationStart(OperationType opType, string filename) oneway;
  void saltpackOperationProgress(OperationType opType, string filename, long bytesComplete, long bytesTotal) oneway;
  void saltpackOperationDone(OperationType opType, string filename) oneway;

}
