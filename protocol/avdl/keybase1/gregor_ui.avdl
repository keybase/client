
@namespace("keybase.1")
protocol gregorUI {
  import idl "github.com/keybase/client/go/protocol/gregor1" as gregor1;

  enum PushReason {
    NONE_0,
    RECONNECTED_1,
    NEW_DATA_2
  }

  void pushState(gregor1.State state, PushReason reason);
  void pushOutOfBandMessages(array<gregor1.OutOfBandMessage> oobm);
}
