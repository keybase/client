
@namespace("keybase.1")
protocol secretUi {
  import idl "passphrase_common.avdl";

  record SecretEntryArg {
    string desc;
    string prompt;
    string err;
    string cancel;
    string ok;
    string reason;
    boolean showTyping;
  }

  record SecretEntryRes {
    string text;
    boolean canceled;
    boolean storeSecret;
  }

  GetPassphraseRes getPassphrase(int sessionID, GUIEntryArg pinentry, union { null, SecretEntryArg } terminal);
}

