@namespace("keybase.1")

protocol session {
  import idl "common.avdl";

  record Session {
    UID uid;
    string username;
    string token;
    KID deviceSubkeyKid;
    KID deviceSibkeyKid;
  }

  Session currentSession(int sessionID);

  record VerifySessionRes {
    UID uid;
    string sid;
    int generated;
    int lifetime;
  }

  VerifySessionRes verifySession(string session);
}
