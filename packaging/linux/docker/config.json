      "dockerfile": "packaging/linux/docker/standard/Dockerfile",

      "_c": "`null` if it has no dependencies, otherwise the key of the image its based on",
      "base": null
    },
    "-slim": {
      "dockerfile": "packaging/linux/docker/slim/Dockerfile",
      "base": ""
    },

    "-alpine": {
