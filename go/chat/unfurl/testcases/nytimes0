  }</script><script data-rh="true" >if (window.NYTD.Abra('DFP_prioritize_top') === '1_prioritize_only') {

  function getSub(user, criteria) {
  const demos = user && user.demographics;
  const email = demos && demos.emailSubscriptions;
  const bundle = demos && demos.bundleSubscriptions;

  // ad params for registered but not subscribed
  if (demos && bundle && criteria) {
    criteria.sub = 'reg';
    if (email && email.length) {
      criteria.em = email.toString().toLowerCase();
    }
    if (demos.wat) {
      criteria.wat = demos.wat.toLowerCase();
    }

    // ad params for subscriber
    if (bundle && bundle.length && bundle[0].bundle) {
      const userDemo = bundle[0];
      criteria.sub = userDemo.bundle.toLowerCase();
      if (userDemo.source) {
        criteria.subsrc = userDemo.source.toLowerCase();
      }
      if (userDemo.promotion) {
        criteria.subprm = userDemo.promotion;
      }
      if (userDemo.in_grace) {
        criteria.grace = userDemo.in_grace.toString();
      }
    }
  }
  return criteria;
}

  function getViewport(deviceType) {
  const viewportMap = {
    smartphone: 'small',
    tablet: 'medium',
    desktop: 'large'
  };

  return deviceType ? viewportMap[deviceType] : viewportMap.desktop;
}

  // replicating some stuff from Jeremy's client/performance
  const noop = function () { return {}; };
  const noopArr = function () { return []; };
  const performance = (typeof window !== 'undefined' && window.performance) || {};

  performance.mark = performance.mark || noop;
  performance.measure = performance.measure || noop;
  performance.getEntriesByName = performance.getEntriesByName || noopArr;
  performance.getEntriesByType = performance.getEntriesByType || noopArr;

  const ADF = {
    trackEvent: function (customEvent) {
      const baseEvent = {
        event: 'impression',
        eventData: {
          type: 'impression',
          trigger: 'ad',
        },
        pageview: {
          ad: {
            slotId: 'top',
            position: 'top',
          },
        }
      };

      // add our ad timings
      Object.keys(customEvent).forEach(function (key) {
        baseEvent.pageview.ad[key] = customEvent[key];
      });

      (window.dataLayer = window.dataLayer || []).push(baseEvent);
    },

    getDeviceType: function () {
      const DeviceType = {
        SMARTPHONE: 'smartphone',
        TABLET: 'tablet',
        DESKTOP: 'desktop',
      };

      let device = DeviceType.SMARTPHONE;

      if (window.matchMedia('(min-width: 740px)').matches) {
        device = DeviceType.TABLET;
      }

      if (window.matchMedia('(min-width: 1150px)').matches) {
        device = DeviceType.DESKTOP;
      }

      return device;
    },

    getAbraDfpVariants: function () {
      const abraData = window.document.documentElement.getAttribute('data-nyt-ab');
      if (abraData) {
        return abraData.split(' ').reduce(function (base, abraVariant) {
          const abraVariantData = abraVariant.split('=');
          const variantName = abraVariantData[0].toLowerCase();
          const variantValues = abraVariantData[1];
          if (variantName.indexOf('dfp') > -1) {
            base.push(variantName + '_' + variantValues);
          }
          return base;
        }, []);
      }
      return '';
    },

    getUserInfo: function () {
      const data = window.userInfoXhrObject
        && window.userInfoXhrObject.responseText !== ''
        && JSON.parse(window.userInfoXhrObject.responseText).data
        || null;
      let user = null;
      if (data && data.user) {
        if (data.user.userInfo) {
          user = {
            demographics: data.user.userInfo.demographics,
          };
        } else {
          user = {};
        }
      }
      return user;
    },

    createTopAdElement: function (id) {
      let adStyles = 'display: block;border-bottom: 1px solid #f3f3f3;background-color: #f7f7f7;padding-top: 15px;padding-bottom: 15px;min-height: 90px;margin: 0;text-align: center;';
      if (window.NYTD.Abra('DFP_ribbon_test') === '1_noRibbon') {
        adStyles += 'margin-top:43px;';
      }
      const ad = document.createElement('div');
      ad.id = id;
      ad.setAttribute('style', adStyles);
      document.body.insertBefore(ad, document.getElementById('app'));
    },

    getTargeting: function (deviceType) {
      let targeting = {"ver":"vi","sov":"2","test":"projectvi","template":"article","hasVideo":false,"pos":"top","si_section":"us","plat":"web","auth":"nicholasfandos","typ":"art","edn":"us","des":"unitedstatespoliticsandgovernm,lawandlegislation,campaignfinance,electionshouseofrepresentative,guncontrol,infrastructurepublicworks,redistrictingandreapportionmen,votingrightsact1965","coll":"thetrumpwhitehouse,us,politics","id":"100000006188863","als_test":"1541004691892","brandsensitive":"false","ledemedsz":"none","prop":"nyt","section":"us","artlen":"medium","emotions":"hate,boredom,hope,optimistic,informed,not_adventurous,not_indulgent,not_interest,not_selfconfident,not_fear,not_amused,not_inthemoodtospend,not_nostalgic,not_sadness,not_love","gscat":"gs_politics,gs_politics_misc,gs_politics_american,gs_law_misc,gs_law,gv_safe"};

      // user info
      const user = this.getUserInfo();
      if (user) {
        targeting = getSub(user, targeting);
      } else {
        targeting.sub = 'anon';
      }

      // GDPR
      if (testCookie('nyt-gdpr') === 1) {
        targeting.gdprUser = true;
      }
      const adTrackingStatus = testCookie('NYT-T');
      if (adTrackingStatus && adTrackingStatus.toLowerCase() === 'out') {
        targeting.noTracking = true;
      }

      targeting.aid = testCookie('nyt-a') || '';
      targeting.abra_dfp = this.getAbraDfpVariants();
      targeting.page_view_id = window.NYTD.PageViewId.current;
      targeting.vp = getViewport(deviceType);

      const topref = document.referrer || null;
      if (topref) {
        targeting.topref = topref;
      }

      return targeting;
    },

    defineAd: function (id) {
      const mark = 'top-timeDefined';
      performance.mark(mark);
      performance.measure(id, 'responseStart', mark);
      let timing = performance.getEntriesByName(id).pop();

      if (timing) {
        this.trackEvent({ timeDefined: Math.round(timing.duration) });
      }

      // define slot
      return window.googletag.defineSlot(
        '/29390238/nyt/us/politics',
        ['fluid', [728, 90], [970, 90], [970, 250], [1605, 300]],
        id
      );
    },

    callAd: function (id) {
      // display slot
      window.googletag.display(id);
      const mark = 'top-timeCalled';
      performance.mark(mark);
      performance.measure(id, 'responseStart', mark);
      const timing = performance.getEntriesByName(id).pop();

      if (timing) {
        this.trackEvent({ timeCalled: Math.round(timing.duration) });
      }
    },

    createTopAd: function () {
      const id = 'top';
      const deviceType = this.getDeviceType();
      if (document.getElementById(id) === null && deviceType !== 'smartphone') {
        // create ad div
        this.createTopAdElement(id);
        const slot = this.defineAd(id);

        let targeting = this.getTargeting(deviceType);
        Object.keys(targeting).forEach(function (key) {
          slot.setTargeting(key, targeting[key]);
        });
        slot.addService(window.googletag.pubads());
        window.googletag.enableServices();

        this.callAd(id);

        // add listeners
        window.googletag.pubads().addEventListener('slotRenderEnded', function (e) {
          if (e.slot.getSlotElementId() === id) {
            const mark = 'top-timeRendered';
            performance.mark(mark);
            performance.measure(id, 'top-timeCalled', mark);
            const timing = performance.getEntriesByName(id).pop();

            if (timing) {
              this.trackEvent({
                timeRendered: Math.round(timing.duration),
                creativeId: e.creativeId || e.sourceAgnosticCreativeId,
                lineItemId: e.lineItemId || e.sourceAgnosticLineItemId,
                isEmpty: e.isEmpty
              });
            }

          }
        }.bind(this));
        window.googletag.pubads().addEventListener('slotOnload', function (e) {
          if (e.slot.getSlotElementId() === id) {
            const mark = 'top-timeLoaded';
            performance.mark(mark);
            performance.measure(id, 'top-timeCalled', mark);
            const timing = performance.getEntriesByName(id).pop();

            if (timing) {
              this.trackEvent({
                timeLoaded: Math.round(timing.duration),
                creativeId: e.creativeId || e.sourceAgnosticCreativeId,
                lineItemId: e.lineItemId || e.sourceAgnosticLineItemId
              });
            }
          }
        }.bind(this));
        window.googletag.pubads().addEventListener('impressionViewable', function (e) {
          if (e.slot.getSlotElementId() === id) {
            const mark = 'top-timeViewed';
            performance.mark(mark);
            performance.measure(id, 'top-timeCalled', mark);
            const timing = performance.getEntriesByName(id).pop();

            if (timing) {
              this.trackEvent({ timeViewed: Math.round(timing.duration) });
            }
          }
        }.bind(this));
      }
    }
  };

  window.googletag = window.googletag || {};
  window.googletag.cmd = window.googletag.cmd || [];
  window.googletag.cmd.push(ADF.createTopAd.bind(ADF));
  }</script>
