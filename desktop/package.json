{
  "name": "keybase",
  "version": "1.0.1",
  "description": "",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "npm run build-dev && npm run start-cold",
    "start-hot": "HOT=true NODE_ENV=development node client.js",
    "start-cold": "NODE_ENV=development electron ./dist/main.bundle.js",
    "build-dev": "NODE_PATH=`pwd`/node_modules NODE_ENV=production NO_SERVER=true DEBUG='express:*' node server.js",
    "build-prod": "NODE_PATH=`pwd`/node_modules NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "package": "NODE_PATH=`pwd`/node_modules NODE_ENV=production node package.js",
    "hot-server": "HOT=true NODE_PATH=`pwd`/node_modules NODE_ENV=production DEBUG='express:*' node server.js",
    "inject-sourcemaps-prod": "a(){ cp \"$1\"/* /Applications/Keybase.app/Contents/Resources/app/desktop/dist; };a",
    "inject-code-prod": "npm run build-prod; cp dist/* /Applications/Keybase.app/Contents/Resources/app/desktop/dist/",
    "start-prod": "/Applications/Keybase.app/Contents/MacOS/Electron",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.4.0",
    "framed-msgpack-rpc": "keybase/node-framed-msgpack-rpc#nojima/keybase-client-changes",
    "getenv": "^0.6.0",
    "immutable": "^3.7.5",
    "lodash": "^4.0.0",
    "marked": "^0.3.5",
    "material-ui": "^0.14.1",
    "menubar": "keybase/menubar",
    "moment": "^2.11.0",
    "purepack": "keybase/purepack#nojima/keybase-client-changes",
    "qrcode-generator": "^1.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-flexbox": "^3.0.0",
    "react-redux": "^4.0.6",
    "react-tap-event-plugin": "^0.2.1",
    "reduce-reducers": "^0.1.1",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.34",
    "del": "^2.2.0",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "0.36.4",
    "electron-rebuild": "^1.0.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eslint-plugin-standard": "^1.3.1",
    "express": "^4.13.3",
    "fs-extra": "^0.26.4",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.1.1",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
